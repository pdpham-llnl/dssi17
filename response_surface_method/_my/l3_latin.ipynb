   "source": [
    "n <- 10\n",
    "l <- (-(n-1)/2):((n-1)/2)\n",
    "\n",
    "m <- 2\n",
    "L <- matrix(NA, nrow=n, ncol=m)\n",
    "\n",
    "for(j in 1:m) L[,j] <- sample(l, n)\n",
    "    \n",
    "U <- matrix(runif(n*m), ncol=m)\n",
    "D <- (L + (n-1)/2 + U)/n"
   ]
  },
